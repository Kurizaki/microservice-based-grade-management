{
  // Logging configuration for your application
  "Logging": {
    "LogLevel": {
      // The default minimum logging level (Information, Warning, Error, etc.)
      "Default": "Information",
      // This applies specifically to logs coming from ASP.NET Core components.
      "Microsoft.AspNetCore": "Warning"
    }
  },

  // Specifies the valid hosts for your application. '*' allows any host.
  "AllowedHosts": "*"
}
